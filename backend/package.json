{
  "name": "youform-clone-backend",
  "version": "1.0.0",
  "description": "Backend API for Youform Clone MVP",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "nodemon --exec ./node_modules/.bin/ts-node src/server.ts",
    "dev:watch": "nodemon --watch src --ext ts --exec ./node_modules/.bin/ts-node src/server.ts",
    "test": "jest",
    "type-check": "tsc --noEmit",
    "migrate": "ts-node src/scripts/migrate.ts run",
    "migrate:rollback": "ts-node src/scripts/migrate.ts rollback",
    "migrate:status": "ts-node src/scripts/migrate.ts status",
    "migrate:generate": "ts-node src/scripts/migrate.ts generate",
    "migrate:validate": "ts-node src/scripts/migrate.ts validate",
    "migrate:reset": "ts-node src/scripts/migrate.ts reset"
  },
  "keywords": [
    "express",
    "mongodb",
    "jwt",
    "form-builder",
    "api"
  ],
  "dependencies": {
    "@slack/web-api": "^7.0.2",
    "@types/pdfkit": "^0.17.2",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "commander": "^11.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dompurify": "^3.2.6",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "google-auth-library": "^9.4.1",
    "googleapis": "^131.0.0",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "json2csv": "^5.0.7",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.3",
    "mongoose-paginate-v2": "^1.7.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.8",
    "pdfkit": "^0.17.1",
    "puppeteer": "^21.6.1",
    "qrcode": "^1.5.3",
    "sharp": "^0.33.1",
    "speakeasy": "^2.0.0",
    "stripe": "^14.9.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5",
    "xss": "^1.0.14"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.0",
    "@types/qrcode": "^1.5.5",
    "@types/speakeasy": "^2.0.10",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "@types/validator": "^13.15.2",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.2.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
